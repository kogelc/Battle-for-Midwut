CC = gcc
NAME = dicowesh
SRC = $(wildcard *.c)
	  $(libmy/wildcard *.c)
INC = $(wildcard *.h)
OBJ = $(SRC:.c=.o)
RM = rm -f
CFLAGS = -W -Wall -Wextra -Werror

$(NAME):	$(OBJ) $(INC)
		$(CC) $(CFLAGS) $(OBJ) -o $(NAME)
		@echo "Compilation complete!"

all:	$(NAME)
clean:
	$(RM) $(OBJ)
	@echo "Cleanup complete!"

fclean:		clean
	$(RM) $(NAME)
	@echo "FCleanup complete!"

re:	fclean all
